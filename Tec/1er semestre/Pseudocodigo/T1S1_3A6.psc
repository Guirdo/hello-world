Proceso T1S1_3A6
	
	Definir peso,euro,dolar,libra,yen Como Real;
	
	Escribir "Ingrese la cantidad en pesos mexicanos: ";
	Leer peso;
	
	euro<-peso/20.65;
	dolar<-peso/18.37;
	libra<-peso/24.57;
	yen<-peso/0.1806;
	
	Escribir peso," pesos mexicanos equivalen a:";
	Escribir "Euros: ",euro;
	Escribir "Dolares: ",dolar;
	Escribir "Libras esterlinas: ",libra;
	Escribir "Yen japones: ",yen;
	
FinProceso
